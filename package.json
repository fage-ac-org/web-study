{
  "name": "vue-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prettier": "prettier --write .",
    "commitlint": "commitlint --from HEAD~1 --to HEAD --verbose"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npx oxlint --fix",
      "npx prettier --write"
    ],
    "*.{css,scss,less}": [
      "npx prettier --write"
    ],
    "*.{json,md}": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^14.0.0",
    "axios": "^1.12.2",
    "element-plus": "^2.11.5",
    "es-toolkit": "^1.40.0",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.3",
    "qs": "^6.14.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.9.1",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "oxlint": "^1.23.0",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.6",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.16.0",
    "vite": "^7.1.10",
    "vite-plugin-fake-server": "^2.2.0",
    "vue-tsc": "^3.1.1"
  }
}
